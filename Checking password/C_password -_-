#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main(void)
{
    int a = 20;
    char str1[a];
    char str2[a];
    fgets(str1, sizeof(str1), stdin);
    fgets(str2, sizeof(str2), stdin);
    int result = strcmp(str1, str2);
    if (result == 1)
    {
        printf("They are not the same");
    }
    else
    {
        printf("They are the same");
    }
}
